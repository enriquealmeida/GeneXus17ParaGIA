<?xml version="1.0" encoding="utf-8"?>
<!-- Actual Action Bar -->
<com.genexus.android.core.controls.GxToolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"

        android:id="@id/action_bar_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_alignParentTop="true"
        android:background="?attr/colorPrimary"

        android:elevation="@dimen/action_bar_default_elevation"

        android:theme="?attr/actionBarCustomTheme"
        app:popupTheme="?attr/actionBarPopupCustomTheme"
    >
</com.genexus.android.core.controls.GxToolbar>
