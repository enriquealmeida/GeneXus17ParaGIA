<CodeExpansions>
	<CodeExpansion Shortcut="table" Name="Table" Toolbox="false">
		<Code Type="dkt" FileName="table.dkt"/>
		<Variables>
			<Variable Name="Rows" Type="System.Int32" Default="2"/>
			<Variable Name="Columns" Type="System.Int32" Default="2"/>
		</Variables>
	</CodeExpansion>
	<CodeExpansion Shortcut="att" Name="Attribute" Toolbox="false">
		<Code Type="dkt" FileName="attribute.dkt"/>
		<Variables>
			<Variable Name="AttRef" DisplayName="Attribute" Type="Artech.Genexus.Common.CustomTypes.AttributeVariableReference, Artech.Genexus.Common"/>
		</Variables>
	</CodeExpansion>
	<CodeExpansion Shortcut="var" Name="Variable" Toolbox="false">
		<Code Type="dkt" FileName="variable.dkt"/>
		<Variables>
			<Variable Name="Name" Type="System.String"/>
			<Variable Name="Type" Type="System.String"/>
		</Variables>
	</CodeExpansion>
	<CodeExpansion Shortcut="wc" Name="Web Component" Toolbox="false">
		<Code Type="dkt" FileName="webcomp.dkt"/>
		<Variables>
			<Variable Name="WebCompRef" DisplayName="Object" Type="Artech.Genexus.Common.CustomTypes.WebComponentReference, Artech.Genexus.Common"/>
		</Variables>
	</CodeExpansion>
