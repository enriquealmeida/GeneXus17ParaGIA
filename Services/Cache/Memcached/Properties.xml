<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Object id="MEMCACHED">
		<Group>
			<Id>GENERAL_GROUP_CACHE</Id>
			<Name>Cache configuration</Name>
			<Description>Cache configuration</Description>
			<Type>Main</Type>
			<Children>
				<Prop>
					<Id>CACHE_PROVIDER_ADDRESS</Id>
					<Name>Cache Location</Name>
					<Description>Distributed Cache server location. Should include host name and port number.  ex: myhost:11211</Description>
					<Type>Text</Type>
					<Default></Default>
					<Options>
						<Option type='Required'/>
					</Options>
				</Prop>
				<Prop>
					<Id>CACHE_PROVIDER_USER</Id>
					<Name>Cache Username</Name>
					<Description>Username for connecting to distributed Cache provider</Description>
					<Type>Text</Type>
				</Prop>
				<Prop>
					<Id>CACHE_PROVIDER_PASSWORD</Id>
					<Name>Cache Password</Name>
					<Description>Password for connecting to distributed Cache provider</Description>
					<Type>Text</Type>
					<Options>
						<Option type='Password'/>
					</Options>
				</Prop>
			</Children>
		</Group>
	</Object>
</Content>