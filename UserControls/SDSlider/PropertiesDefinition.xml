<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Object id="SDSlider">
		<Group>
			<Name>General</Name>
			<Type>Main</Type>
			<Children>
				<Prop>
					<Id>AttID</Id>
					<Name>Attribute</Name>
					<Type>Custom</Type>
					<CustomType>Artech.Genexus.Common.CustomTypes.ControlAttVarReference, Artech.Genexus.Common</CustomType>
					<Metadata>
						<Value name="FlagSetValueMethod">SetAttribute</Value>
						<Value name="FlagGetValueMethod">GetAttribute</Value>
						<Value name="FlagScope">DesignTime</Value>
						<Value name="FlagDataTypeFilter">numeric</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>MinValue</Id>
					<Name>MinValue</Name>
					<Description>Minimum value for the Slider</Description>
					<Type>Text</Type>
					<Default>1</Default>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name='FlagDisplayName'>Minimum Value</Value>
						<Value name='FlagRuntimeName'>MinValue</Value>
						<Value name="FlagSpecDatatype">int</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>MaxValue</Id>
					<Name>MaxValue</Name>
					<Description>Maximum value for the Slider</Description>
					<Type>Text</Type>
					<Default>5</Default>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name='FlagDisplayName'>Maximum Value</Value>
						<Value name='FlagRuntimeName'>MaxValue</Value>
						<Value name="FlagSpecDatatype">int</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>Step</Id>
					<Name>Step</Name>
					<Description>The Step property should be used when the values of the control can take some discrete values only. For example if valid values are {10, 20, 30} set the Min Value to 10, the Max Value to 30, and the Step to 10. If the Step is 0, then any intermediate value is valid.</Description>
					<Type>Text</Type>
					<Default>0</Default>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name="FlagSpecDatatype">int</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>DisplayValue</Id>
					<Name>DisplayValue</Name>
					<Description>Indicates wether the control should display a bubble with the current value upon interaction.</Description>
					<Type>Boolean</Type>
					<Default>false</Default>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name="FlagScope">DesignTime</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>ValueThemeClass</Id>
					<Name>ValueThemeClass</Name>
					<Description>The theme class for the current value's bubble</Description>
					<Type>Text</Type>
					<Default></Default>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name="FlagScope">DesignTime</Value>
					</Metadata>
					<Options>
						<Option type="Not Show" />
					</Options>
				</Prop>
				<Prop>
					<Id>MinValueImageIndicator</Id>
					<Name>MinValueImage</Name>
					<Description>Minimum Value Image Indicator</Description>
					<Type>Custom</Type>
					<CustomType>Artech.Genexus.Common.CustomTypes.LocalizableImageReference, Artech.Genexus.Common</CustomType>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name="FlagSpecDatatype">Image</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>MaxValueImageIndicator</Id>
					<Name>MaxValueImage</Name>
					<Description>Maximum Value Image Indicator</Description>
					<Type>Custom</Type>
					<CustomType>Artech.Genexus.Common.CustomTypes.LocalizableImageReference, Artech.Genexus.Common</CustomType>
					<Metadata>
						<Value name='FlagControlInfoProp'></Value>
						<Value name="FlagSpecDatatype">Image</Value>
					</Metadata>
				</Prop>
			</Children>
		</Group>
	</Object>
</Content>
