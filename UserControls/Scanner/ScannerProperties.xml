<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Object id="Scanner">
    <Group>
      <Name>General</Name>
      <Type>Main</Type>
      <Childs>
        <Prop>
          <Id>AttID</Id>
          <Name>Attribute</Name>
          <Type>Custom</Type>
          <CustomType>Artech.Genexus.Common.CustomTypes.ControlAttVarReference, Artech.Genexus.Common</CustomType>
          <Metadata>
            <Value name="FlagSetValueMethod">SetAttribute</Value>
            <Value name="FlagGetValueMethod">GetAttribute</Value>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
        </Prop>
        <Prop>
          <Id>BarcodeTypes</Id>
          <Name>Barcode Types</Name>
          <Description>BarcodeType comma-separated list.</Description>
          <Type>Text</Type>
          <Metadata>
            <Value name='FlagControlInfoProp'></Value>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
        </Prop>
        <Prop>
          <Id>DisplayMode</Id>
          <Name>Display mode</Name>
          <Description>When set to As Prompt, it shows an indicator that presents a new screen with the camera viewport. When Inline is selected, the camera viewport is presented inside the Panelâ€™s layout.</Description>
          <Type>Combo</Type>
          <Default>AsPrompt</Default>
          <Metadata>
            <Value name='FlagControlInfoProp'></Value>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
          <Values>
            <Value id="AsPrompt" desc="As prompt" />
            <Value id="Inline" desc="Inline" />
          </Values>
        </Prop>
        <Prop>
          <Id>OperationMode</Id>
          <Name>Operation mode</Name>
          <Description>When set to Single Read, the control reads the first code and then stops scanning. When Continuous Read is selected, it reads all the codes it can while the control is visible, non-stop.</Description>
          <Type>Combo</Type>
          <Default>SingleRead</Default>
          <Metadata>
            <Value name='FlagControlInfoProp'></Value>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
          <Values>
            <Value id="SingleRead" desc="Single read" />
            <Value id="ContinuousRead" desc="Continuous read" />
          </Values>
        </Prop>
        <Prop>
          <Id>Beep</Id>
          <Name>Beep on each read</Name>
          <Description>When set to True, it will make a beep after each read.</Description>
          <Type>Boolean</Type>
          <Default>false</Default>
          <Metadata>
            <Value name='FlagControlInfoProp'></Value>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
        </Prop>
      </Childs>
    </Group>
  </Object>
</Content>
