<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Object id="Tab">
    <Group>
      <Name>General</Name>
      <Type>Main</Type>
      <Children>
        <Prop>
          <Id>ActivePage</Id>
          <Name>ActivePage</Name>
          <Type>Integer</Type>
          <Metadata>
            <Value name="FlagScope">Runtime</Value>
          </Metadata>
          <Description>Active page index (first tab index is 1).</Description>
        </Prop>
        <Prop>
          <Id>ActivePageControlName</Id>
          <Name>ActivePageControlName</Name>
          <Type>Text</Type>
          <Metadata>
            <Value name="FlagScope">Runtime</Value>
          </Metadata>
          <Description>Active page control name.</Description>
        </Prop>
        <Prop>
          <Id>PageCount</Id>
          <Name>PageCount</Name>
          <Type>Integer</Type>
          <Description>Number of tab pages.</Description>
        </Prop>
        <Prop>
          <Id>Class</Id>
          <Name>Class</Name>
          <Type>Text</Type>
          <Default>Tab</Default>
        </Prop>
        <Prop>
          <Id>HistoryManagement</Id>
          <Name>HistoryManagement</Name>
          <Type>Boolean</Type>
          <Default>false</Default>
          <Metadata>
            <Value name="FlagScope">Designtime</Value>
          </Metadata>
          <Description>True to enable automatic history management. When enabled, every time a user clicks on a tab, a new entry is added to the browser History. When the user presses the browser's back button, the previous tab is activated, and the TabChange event is fired.</Description>
        </Prop>
      </Children>
    </Group>
  </Object>
</Content>