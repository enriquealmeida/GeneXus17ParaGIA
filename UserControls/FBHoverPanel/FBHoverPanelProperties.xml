<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Object id="FBHoverPanel">
    <Group>
      <Name>General</Name>
      <Type>Main</Type>
      <Childs>
        <Prop>
          <Id>MaxWidth</Id>
          <Name>MaxWidth</Name>
          <Type>Text</Type>
          <Default>500</Default>
        </Prop>
        <Prop>
          <Id>MaxHeight</Id>
          <Name>MaxHeight</Name>
          <Type>Text</Type>
          <Default>500</Default>
        </Prop>
        <Group>
          <Name>ActionSection</Name>
          <Childs>
          	<Prop>
          		<Id>AutoHide</Id>
          		<Name>AutoHide</Name>
          		<Type>Boolean</Type>
          		<Default>true</Default>
        		</Prop>
        		<Prop>
		        	<Id>AutoFit</Id>
		          <Name>AutoFit</Name>
		          <Type>Combo</Type>
		          <Default>true</Default>
		          <Metadata />
		          <Values>
		            <Value id="true" desc="True" />
		            <Value id="false" desc="False" />
		          </Values>
        		</Prop>
          </Childs>
        </Group>
        <Group>
          <Name>DropDownSection</Name>
          <Childs>
			<Prop>
				<Id>FadeInSpeed</Id>
				<Name>FadeInSpeed</Name>
				<Type>Integer</Type>
				<Default>300</Default>
				<Metadata/>
			</Prop>	
          	<Prop>
		          <Id>Alignment</Id>
		          <Name>Alignment</Name>
		          <Type>Combo</Type>
		          <Default>right</Default>
		          <Metadata />
		          <Values>
		            <Value id="left" desc="Left" />
		            <Value id="right" desc="Right" />
		          </Values>
        		</Prop>
        		<Prop>
		          <Id>DropDownSectionVisible</Id>
		          <Name>DropDownSectionVisible</Name>
		          <Type>Combo</Type>
		          <Default>true</Default>
		          <Metadata />
		          <Values>
		            <Value id="true" desc="True" />
		            <Value id="false" desc="False" />
		          </Values>
        		</Prop>
          </Childs>
        </Group>  	
      </Childs>
    </Group>
  </Object>
</Content>