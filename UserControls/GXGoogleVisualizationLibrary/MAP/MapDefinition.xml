<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="4678 $">
  <Object id="GoogleMapControl">
    <Group>
      <Name>General</Name>
      <Type>Main</Type>
      <Childs>
        <Prop>
          <Id>Title</Id>
          <Name>Title</Name>
          <Type>Text</Type>
          <Default>Map Title</Default>
          <Metadata />
        </Prop>
        <Prop>
          <Id>Width</Id>
          <Name>Width</Name>
          <Type>Text</Type>
          <Default>500px</Default>
          <Metadata />
        </Prop>
        <Prop>
          <Id>Height</Id>
          <Name>Height</Name>
          <Type>Text</Type>
          <Default>350px</Default>
          <Metadata />
        </Prop>
        <Prop>
          <Id>Provider</Id>
          <Name>Provider</Name>
          <Type>Combo</Type>
          <Default>GOOGLE</Default>
          <Metadata>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
          <Values>
            <Value id="GOOGLE" desc="Google Maps" />
			<Value id="BAIDU" desc="Baidu Maps" />
          </Values>
          <Description>Determines the map provider (Google, Baidu, etc).</Description>
        </Prop>
        <Prop>
          <Id>Type</Id>
          <Name>Type</Name>
          <Type>Combo</Type>
          <Default>G_NORMAL_MAP</Default>
          <Metadata />
          <Values>
            <Value id="G_NORMAL_MAP" desc="Map" />
            <Value id="G_SATELLITE_MAP" desc="Satellital Photo" />
            <Value id="G_HYBRID_MAP" desc="Hybrid" />
            <Value id="G_TERRAIN_MAP" desc="Terrain" />
          </Values>
        </Prop>
        <Prop>
          <Id>City</Id>
          <Name>City</Name>
          <Type>Combo</Type>
          <Default>-34.90,-56.11</Default>
          <Metadata />
          <Values>
            <Value id="-34.90,-56.11" desc="Montevideo" />
            <Value id="-25.31,-57.56" desc="Asuncion" />
            <Value id="41.36,2.12" desc="Barcelona" />
            <Value id="39.55,116.25" desc="Beijing" />
            <Value id="4.35,-74.04" desc="Bogota" />
            <Value id="-34.65,-58.40" desc="Buenos Aires" />
            <Value id="10.48,-66.87" desc="Caracas" />
            <Value id="41.85,-87.55" desc="Chicago" />
            <Value id="-31.40,-64.15" desc="Cordoba - Arg" />
            <Value id="14.58,-90.56" desc="Guatemala" />
            <Value id="-2.20,-79.90" desc="Guayaquil" />
            <Value id="23.15,-82.23" desc="La Habana" />
            <Value id="-26.19,27.80" desc="Johannesburg" />
            <Value id="-16.50,-68.15" desc="La Paz" />
            <Value id="-12.0,-77.20" desc="Lima" />
            <Value id="40.26,-3.42" desc="Madrid" />
            <Value id="19.45,-99.12" desc="Mexico City" />
            <Value id="48.86,2.38" desc="Paris" />
            <Value id="8.58,-79.32" desc="Panama city" />
            <Value id="-30.01,-51.13" desc="Porto Alegre" />
			<Value id="18.533333,-72.333333" desc="Port-au-Prince" />
            <Value id="-22.57,-43,12" desc="Rio de Janeiro" />
            <Value id="41.92,12.53" desc="Roma" />
            <Value id="-23.56,-46.64" desc="Sao Paulo" />
            <Value id="9.59,-84.04" desc="San Jose " />
			<Value id="13.69, -89.19" desc="San Salvador " />
            <Value id="-33.52,-70.64" desc="Santiago de Chile" />
			<Value id="31.2,121.5" desc="Shanghai" />
            <Value id="14.04,-87.13" desc="Tegucigalpa" />
            <Value id="35.69, 139.70" desc="Tokyo" />
            <Value id="0,0" desc="Specific Coordinate" />
          </Values>
          <Description>Sets a city to position the map on it.</Description>
        </Prop>
        <Prop>
          <Id>Latitude</Id>
          <Name>Latitude</Name>
          <Type>Text</Type>
          <Default />
          <Metadata />
          <Description>Determines a specific latitude to center the map.</Description>
        </Prop>
        <Prop>
          <Id>Longitude</Id>
          <Name>Longitude</Name>
          <Type>Text</Type>
          <Default />
          <Metadata />
          <Description>Determines a specific longitude to center the map.</Description>
        </Prop>
        <Prop>
          <Id>Precision</Id>
          <Name>Precision</Name>
          <Type>Integer</Type>
          <Default>8</Default>
          <Metadata />
        </Prop>
		<Prop>
          <Id>Style</Id>
          <Name>Style</Name>
          <Type>Combo</Type>
          <Default>GStyle_Standard</Default>
		  <Values>
                <Value id="GStyle_Standard" desc="Standard" />
                <Value id="GStyle_Silver" desc="Silver" />
				<Value id="GStyle_Retro" desc="Retro" />
                <Value id="GStyle_Dark" desc="Dark" />
                <Value id="GStyle_Nigth" desc="Nigth" />
                <Value id="GStyle_Aubergine" desc="Aubergine" />
              </Values>
          <Metadata />
          <Description>Specifies a Style Map.</Description>
        </Prop>
        <Group>
          <Name>Controls</Name>
          <Childs>
            <Prop>
              <Id>GSmall</Id>
              <Name>Small_Control</Name>
              <Type>Combo</Type>
              <Default>GSmall_True</Default>
              <Metadata />
              <Values>
                <Value id="GSmall_True" desc="True" />
                <Value id="GSmall_False" desc="False" />
              </Values>
              <Description>Displays the small control on the Map control.</Description>
            </Prop>
            <Prop>
              <Id>GType</Id>
              <Name>Type_Control</Name>
              <Type>Combo</Type>
              <Default>GType_False</Default>
              <Metadata />
              <Values>
                <Value id="GType_True" desc="True" />
                <Value id="GType_False" desc="False" />
              </Values>
              <Description>Determines whether Terrain, Satellite or Hybrid Map is used.</Description>
            </Prop>
            <Prop>
              <Id>Scale</Id>
              <Name>Scale_Control</Name>
              <Type>Combo</Type>
              <Default>GScale_False</Default>
              <Metadata />
              <Values>
                <Value id="GScale_True" desc="True" />
                <Value id="GScale_False" desc="False" />
              </Values>
              <Description>Sets whether or not to show the Scale control on the Map control.</Description>
            </Prop>
            <Group>
              <Name>Google</Name>
              <Childs>
                <Prop>
                  <Id>Overview</Id>
                  <Name>OverView_Control</Name>
                  <Type>Combo</Type>
                  <Default>GOverviewMap_False</Default>
                  <Metadata />
                  <Values>
                    <Value id="GOverviewMap_True" desc="True" />
                    <Value id="GOverviewMap_False" desc="False" />
                  </Values>
              <Description>Determines whether or not to show the OverView control on the Map control.</Description>
                </Prop>
                <Prop>
                  <Id>SmallZoom</Id>
                  <Name>Small_Zoom_Control</Name>
                  <Type>Combo</Type>
                  <Default>GSmallZoom_False</Default>
                  <Metadata />
                  <Values>
                    <Value id="GSmallZoom_True" desc="True" />
                    <Value id="GSmallZoom_False" desc="False" />
                  </Values>
              <Description>Sets whether or not to show the Small zoom control on the Google Map control.</Description>
                </Prop>
                <Prop>
                  <Id>Large</Id>
                  <Name>Large_Control</Name>
                  <Type>Combo</Type>
                  <Default>GLarge_False</Default>
                  <Metadata />
                  <Values>
                    <Value id="GLarge_True" desc="True" />
                    <Value id="GLarge_False" desc="False" />
                  </Values>
                  <Description>Sets whether or not to show the Large control, on the Map control.</Description>
                </Prop>
                <Prop>
                  <Id>MapType_Control_Style</Id>
                  <Name>MapType_Control_Style</Name>
                  <Type>Combo</Type>
                  <Default>DEFAULT</Default>
                  <Metadata />
                  <Values>
                    <Value id="DEFAULT" desc="Default" />
                    <Value id="DROPDOWN_MENU" desc="DropDown Menu" />
                    <Value id="HORIZONTAL_BAR" desc="Horizontal Bar" />
                  </Values>
                  <Description>Sets whether or not to show the Map Type control, on the Map control.</Description>
                </Prop>
                <Prop>
                  <Id>Navigation_Control_Style</Id>
                  <Name>Navigation_Control_Style</Name>
                  <Type>Combo</Type>
                  <Default>DEFAULT</Default>
                  <Metadata />
                  <Values>
                    <Value id="DEFAULT" desc="Default" />
                    <Value id="SMALL" desc="Small" />
                    <Value id="ANDROID" desc="Android" />
                    <Value id="ZOOM_PAN" desc="Zoom Pan" />
                  </Values>
                  <Description>Sets whether or not to show the Navigation control Style, on the Map control.</Description>
                </Prop>
                <Prop>
                  <Id>ScrollWheel</Id>
                  <Name>ScrollWheel</Name>
                  <Type>Boolean</Type>
                  <Default>True</Default>
                  <Metadata />
                  <Description>Enables/disables Scroll wheel on Google provider Map control.</Description>
                </Prop>
			   </Childs>
             </Group>
          </Childs>
        </Group>
        <Group>
          <Name>DataBinding</Name>
          <Childs>
            <Prop>
              <Id>GxMap</Id>
              <Name>GxMap</Name>
              <Type>Custom</Type>
              <CustomType>Artech.Genexus.Common.CustomTypes.ControlAttVarReference, Artech.Genexus.Common</CustomType>
              <Metadata>
                <Value name="FlagDataTypeFilter">GxMap</Value>
                <Value name="FlagSetValueMethod">SetData</Value>
                <Value name="FlagGetValueMethod">GetData</Value>
                <Value name="FlagScope">DesignTime</Value>
                <Value name="FlagHideVariables">False</Value>
                <Value name="FlagHideAttributes">True</Value>
              </Metadata>
              <Description>Structured data type to be associated with the map.</Description>
            </Prop>
            <Prop>
              <Id>ClickLatitude</Id>
              <Name>ClickLatitude</Name>
              <Type>Custom</Type>
              <CustomType>Artech.Genexus.Common.CustomTypes.ControlAttVarReference, Artech.Genexus.Common</CustomType>
              <Metadata>
                <Value name="FlagDataTypeFilter">Character</Value>
                <Value name="FlagSetValueMethod">SetClickLatitude</Value>
                <Value name="FlagGetValueMethod">GetClickLatitude</Value>
                <Value name="FlagScope">DesignTime</Value>
                <Value name="FlagHideVariables">False</Value>
                <Value name="FlagHideAttributes">True</Value>
              </Metadata>
              <Description>Gets the Latitude value when clicking on the map control.</Description>
            </Prop>
            <Prop>
              <Id>ClickLongitude</Id>
              <Name>ClickLongitude</Name>
              <Type>Custom</Type>
              <CustomType>Artech.Genexus.Common.CustomTypes.ControlAttVarReference, Artech.Genexus.Common</CustomType>
              <Metadata>
                <Value name="FlagDataTypeFilter">Character</Value>
                <Value name="FlagSetValueMethod">SetClickLongitude</Value>
                <Value name="FlagGetValueMethod">GetClickLongitude</Value>
                <Value name="FlagScope">DesignTime</Value>
                <Value name="FlagHideVariables">False</Value>
                <Value name="FlagHideAttributes">True</Value>
              </Metadata>
              <Description>Gets the Longitude value when clicking on the map control.</Description>
            </Prop>
            <Prop>
              <Id>InformationControl</Id>
              <Name>InformationControl</Name>
              <Type>Text</Type>
              <Default>ControlName</Default>
              <Metadata />
              <Description>It associates a name for the control binding.</Description>
            </Prop>
          </Childs>
        </Group>
        <Group>
          <Name>Google Provider</Name>
          <Childs>
            <Prop>
              <Id>Icon</Id>
              <Name>Icon</Name>
              <Type>Combo</Type>
              <Default>Default</Default>
              <Metadata />
              <Values>
                <Value id="Default" desc="Default" />
                <Value id="Blue" desc="Blue" />
                <Value id="Green" desc="Green" />
                <Value id="Orange" desc="Orange" />
                <Value id="Pink" desc="Pink" />
                <Value id="Red" desc="Red" />
                <Value id="BlueC" desc="BlueCircle" />
                <Value id="GreenC" desc="GreenCircle" />
                <Value id="YellowC" desc="YellowCircle" />
              </Values>
              <Description>Determines the default icon for markers on map control.</Description>
            </Prop>
			<Prop>
				<Id>IconWidth</Id>
				<Name>IconWidth</Name>
				<Type>Integer</Type>
				<Default>20</Default>
				<Metadata />
				<Description>Determines the default icon width for markers on map control.</Description>
			</Prop>
			<Prop>
				<Id>IconHeight</Id>
				<Name>IconHeight</Name>
				<Type>Integer</Type>
				<Default>32</Default>
				<Metadata />
				<Description>Determines the default icon height for markers on Map control.</Description>
			</Prop>
			<Prop>
				<Id>AnchorLeft</Id>
				<Name>AnchorLeft</Name>
				<Type>Integer</Type>
				<Default>10</Default>
				<Metadata />
				<Description>Sets the anchor Left icon for Google Map control.</Description>
			</Prop>
			<Prop>
				<Id>AnchorTop</Id>
				<Name>AnchorTop</Name>
				<Type>Integer</Type>
				<Default>32</Default>
				<Metadata />
				<Description>Sets the anchor Top icon for Google Map control.</Description>
			</Prop>
			<Prop>
				<Id>GoogleApiKey</Id>
				<Name>GoogleApiKey</Name>
				<Type>Text</Type>
				<Default />
				<Metadata />
				<Description>Sets a Google API Key to display the Google Map control at runtime.</Description>
			</Prop>
			<Group>
              <Name>KML</Name>
              <Childs> 
				<Prop>
                  <Id>KML</Id>
                  <Name>KML</Name>
                  <Type>Boolean</Type>
                  <Metadata />
                  <Values>
                    <Value id="true" desc="true" />
                  </Values>
                  <Description>Enables or not KMl file for Map control.</Description>
                </Prop>
				<Prop>
                  <Id>KMLURL</Id>
                  <Name>KMLURL</Name>
                  <Type>Text</Type>
				  <Default></Default>
                  <Metadata/>
                  <Description>It determines the KMl Url file, for a Map control.</Description>
				</Prop>
	           </Childs>
            </Group>
            <Prop>
              <Id>OpenLinksInNewWindow</Id>
              <Name>OpenLinksInNewWindow</Name>
              <Type>Combo</Type>
              <Default>OpenNew_True</Default>
              <Metadata />
              <Values>
                <Value id="OpenNew_True" desc="true" />
                <Value id="OpenNew_False" desc="false" />
              </Values>
              <Description>Sets that any link on the map will be opened in a new window.</Description>
            </Prop>
            <Group>
              <Name>GetCoordinates</Name>
              <Childs>
                <Prop>
                  <Id>getIcon</Id>
                  <Name>getIcon</Name>
                  <Type>Combo</Type>
                  <Default>Orange</Default>
                  <Metadata />
                  <Values>
                    <Value id="Blue" desc="Blue" />
                    <Value id="Green" desc="Green" />
                    <Value id="Orange" desc="Orange" />
                    <Value id="Pink" desc="Pink" />
                    <Value id="Red" desc="Red" />
                    <Value id="BlueC" desc="BlueCircle" />
                    <Value id="GreenC" desc="GreenCircle" />
                    <Value id="YellowC" desc="YellowCircle" />
                  </Values>
                  <Description>Determines the Marker icon color when clicking on the map control.</Description>
                </Prop>
                <Prop>
                  <Id>Onclick</Id>
                  <Name>Onclick</Name>
                  <Type>Combo</Type>
                  <Default>add_Marker</Default>
                  <Metadata>
                    <Value name="FlagScope">DesignTime</Value>
                  </Metadata>
                  <Values>
                    <Value id="none" desc="none" />                    
                    <Value id="add_Marker" desc="Add Marker" />
                    <Value id="getvalue" desc="Get Value" />                    
                  </Values>
                  <Description>Determines the on click behavior on the map.</Description>
                </Prop>
                <Prop>
                  <Id>CenterWhenClick</Id>
                  <Name>CenterWhenClick</Name>
                  <Type>Boolean</Type>
                  <Default>True</Default>
                  <Metadata />
                  <Description>Determines whether to center the map when clicked.</Description>
                </Prop>
                <Prop>
                  <Id>Clear_Overlay</Id>
                  <Name>Clear_Overlay</Name>
                  <Type>Boolean</Type>
                  <Metadata />
                  <Values>
                    <Value id="true" desc="true" />
                  </Values>
                  <Description>Clears overlay when clicking the marker on the map control.</Description>
                </Prop>
              </Childs>
            </Group>
          </Childs>
        </Group>
		<Group>
          <Name>Baidu Provider</Name>
		  <Childs>
		         <Prop>
                  <Id>BaiduKey</Id>
                  <Name>BaiduKey</Name>
                  <Type>Text</Type>
				  <Default>41e051de8c41e0a4532e367c6b0e12fc</Default>
                  <Metadata/>
                </Prop>
				</Childs>
		</Group>
		<Group>
          <Name>Routing</Name>
          <Childs>
            <Prop>
              <Id>Travel_Mode</Id>
              <Name>Travel_Mode</Name>
              <Type>Combo</Type>
			  <Default>1</Default>
              <Metadata />
              <Values>
                <Value id="1" desc="Driving" />
                <Value id="2" desc="Walking" />
              </Values>
              <Description>Determines the travel mode when routing on map control.</Description>
            </Prop>
          </Childs>
        </Group>
      </Childs>
    </Group>
  </Object>
</Content>