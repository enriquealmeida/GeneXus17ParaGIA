<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Object id="MapPinImage">
    <Group>
      <Name>General</Name>
      <Type>Main</Type>
      <Children>
        <Prop>
          <Id>PinHeight</Id>
          <Name>Pin Image Height</Name>
          <Description>The height (in dips) of the Pin Image</Description>
          <Type>Integer</Type>
          <Default>0</Default>
          <Metadata>
            <Value name='FlagSendToCss'></Value>
            <Value name='FlagExport'>true</Value>
            <Value name='FlagDesignSystemName'>gx-pin-height</Value>
          </Metadata>
        </Prop>
        <Prop>
          <Id>PinWidth</Id>
          <Name>Pin Image Width</Name>
          <Description>The width (in dips) of the Pin Image</Description>
          <Type>Integer</Type>
          <Default>0</Default>
          <Metadata>
            <Value name='FlagSendToCss'></Value>
            <Value name='FlagExport'>true</Value>
            <Value name='FlagDesignSystemName'>gx-pin-width</Value>
          </Metadata>
        </Prop>
        <Prop>
          <Id>PinScaleType</Id>
          <Name>Pin Image Scale Type</Name>
          <Description>The scale type applied to the Pin Image</Description>
          <Type>Combo</Type>
          <Default></Default>
          <Metadata>
            <Value name='FlagSendToCss'></Value>
            <Value name='FlagExport'>true</Value>
            <Value name="FlagScope">DesignTime</Value>
            <Value name='FlagControlInfoProp'></Value>
            <Value name='FlagDesignSystemName'>gx-pin-scale-type</Value>
          </Metadata>
          <Values>
            <Value id="" desc="" />
            <Value id="originalSize" desc="No Scale">
              <Meta name='FlagDesignSystemName'>original</Meta>
            </Value>
            <Value id="tile" desc="Tile">
              <Meta name='FlagDesignSystemName'>tile</Meta>
            </Value>
            <Value id="scaleToFill" desc="Fill">
              <Meta name='FlagDesignSystemName'>fill</Meta>
            </Value>
            <Value id="scaleAspectFillKeepingAspect" desc="Fill Keeping Aspect Ratio">
              <Meta name='FlagDesignSystemName'>fill-keeping-aspect</Meta>
            </Value>
            <Value id="scaleToFitKeepingAspect" desc="Fit">
              <Meta name='FlagDesignSystemName'>fit</Meta>
            </Value>
          </Values>
        </Prop>
      </Children>
    </Group>
  </Object>
</Content>