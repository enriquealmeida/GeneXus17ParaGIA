<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Object id="FlexGrid">
		<Group>
			<Name>General</Name>
			<Type>Main</Type>
			<Children>
				<Prop>
					<Id>flexDirection</Id>
					<Name>FlexDirection</Name>
					<Type>Combo</Type>
					<Default>row</Default>
					<Description>This attribute determines the direction of the main axis (and the cross axis, perpendicular to the main axis). The direction children items are placed inside the Flexbox layout.</Description>
					<Values>
						<Value id="row" desc="Row" />
						<Value id="row-reverse" desc="Row Reverse" />
						<Value id="column" desc="Column" />
						<Value id="column-reverse" desc="Column Reverse" />
					</Values>
					<Metadata>
						<Value name='FlagWebLayoutProperty'></Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>flexWrap</Id>
					<Name>FlexWrap</Name>
					<Type>Combo</Type>
					<Default>nowrap</Default>
					<Description>This attribute controls whether the flex container is single-line or multi-line, and the direction of the cross axis.</Description>
					<Values>
						<Value id="nowrap" desc="No Wrap" />
						<Value id="wrap" desc="Wrap" />
						<Value id="wrap-reverse" desc="Wrap Reverse" />
					</Values>
					<Metadata>
						<Value name='FlagWebLayoutProperty'></Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>justifyContent</Id>
					<Name>JustifyContent</Name>
					<Type>Combo</Type>
					<Default>flex-start</Default>
					<Description>This attribute controls the alignment along the main axis.</Description>
					<Values>
						<Value id="flex-start" desc="Flex Start" />
						<Value id="flex-end" desc="Flex End" />
						<Value id="center" desc="Center" />
						<Value id="space-between" desc="Space Between" />
						<Value id="space-around" desc="Space Around" />
					</Values>
					<Metadata>
						<Value name='FlagWebLayoutProperty'></Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>alignItems</Id>
					<Name>AlignItems</Name>
					<Type>Combo</Type>
					<Default>stretch</Default>
					<Description>This attribute controls the alignment along the cross axis.</Description>
					<Values>
						<Value id="stretch" desc="Stretch" />
						<Value id="flex-start" desc="Flex Start" />
						<Value id="flex-end" desc="Flex End" />
						<Value id="center" desc="Center" />
						<Value id="baseline" desc="Baseline" />
					</Values>
					<Metadata>
						<Value name='FlagWebLayoutProperty'></Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>alignContent</Id>
					<Name>AlignContent</Name>
					<Type>Combo</Type>
					<Default>stretch</Default>
					<Description>This attribute controls the alignment of the flex lines in the flex container.</Description>
					<Values>
						<Value id="stretch" desc="Stretch" />
						<Value id="flex-start" desc="Flex Start" />
						<Value id="flex-end" desc="Flex End" />
						<Value id="center" desc="Center" />
						<Value id="space-between" desc="Space Between" />
						<Value id="space-around" desc="Space Around" />
					</Values>
					<Metadata>
						<Value name='FlagWebLayoutProperty'></Value>
					</Metadata>
				</Prop>
			</Children>
		</Group>
	</Object>
</Content>