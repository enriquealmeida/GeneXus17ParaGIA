<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Object id="WWP.DatePicker">
    <Group>
      <Name>General</Name>
      <Type>Main</Type>
      <Childs>
        <Prop>
          <Id>MinYear</Id>
          <Name>MinYear</Name>
          <Type>Integer</Type>
          <Default>1970</Default>
          <Metadata>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
        </Prop>
        <Prop>
          <Id>MaxYear</Id>
          <Name>MaxYear</Name>
          <Type>Integer</Type>
          <Default>2040</Default>
          <Metadata>
            <Value name="FlagScope">DesignTime</Value>
          </Metadata>
        </Prop>
        <Prop>
          <Id>Style</Id>
          <Name>Style</Name>
          <Type>Combo</Type>
          <Default>Light</Default>
          <Metadata />
          <Values>
            <Value id="Light" desc="Light" />
            <Value id="Dark" desc="Dark" />
          </Values>
        </Prop>
      </Childs>      
    </Group>    
  </Object>
</Content>