		idJDBC_DRIVER_ORA_THIN 		"oracle.jdbc.driver.OracleDriver",
		idJDBC_DRIVER_400_TOOLBOX 	"com.ibm.as400.access.AS400JDBCDriver",
		idJDBC_DRIVER_400_NATIVE	 "com.ibm.db2.jdbc.app.DB2Driver",
		idJDBC_DRIVER_DB2_TYPE2		"COM.ibm.db2.jdbc.app.DB2Driver",
		idJDBC_DRIVER_DB2_THIN	  	"COM.ibm.db2.jdbc.net.DB2Driver",
		idJDBC_DRIVER_MERANT 	  	"intersolv.jdbc.sequelink.SequeLinkDriver",
		idJDBC_DRIVER_MERANT_SQL	"com.merant.datadirect.jdbc.sqlserver.SQLServerDriver",
		idJDBC_DRIVER_MERANT_DB2	"com.merant.datadirect.jdbc.db2.DB2Driver",
		idJDBC_DRIVER_POSTGRES_ORG	"org.postgresql.Driver"
	]
	COMBO
	ID=idJDBC_DRIVER
	NAME="JDBC Driver"
	DESC="JDBC Driver"
	APPLY=DS_ORACLE,DS_INFORMIX,DS_SQLSERVER,DS_DB2400,DS_DB2COMMONSERVERS,DS_AS400NATIVE,DS_POSTGRESQL
	DATA=[
		{$idJDBC_DRIVER_POSTGRES_ORG} "PostgreSQL JDBC Driver (Type 4)",
		{$idJDBC_DRIVER_SQL_MS} "Microsoft JDBC Driver (Type 4)",
		{$idJDBC_DRIVER_SQL_INET} "I-Net Una/Sprinta (Type 4)",
		{$idJDBC_DRIVER_ORA_INET} "I-Net Auguro (Type 4)",
		{$idJDBC_DRIVER_SQL_WEBLOGIC} "WebLogic jDriver (Type 4)",
		{$idJDBC_DRIVER_IFX_WEBLOGIC} "WebLogic jDriver (Type 4)" ,
		{$idJDBC_DRIVER_IFX_THIN} "Informix Thin (Type 4)",
		{$idJDBC_DRIVER_ORA_THIN} "Oracle Thin (Type 4)",
		{$idJDBC_DRIVER_400_TOOLBOX} "AS/400 Toolbox (Type 4)",
		{$idJDBC_DRIVER_400_NATIVE} "AS/400 Native (Type 2)",
		{$idJDBC_DRIVER_DB2_THIN} "IBM DB2 Driver (Type 4)",
		{$idJDBC_DRIVER_DB2_TYPE2} "IBM DB2 Driver (Type 2)",
		{$idJDBC_DRIVER_MERANT} "Merant SequeLink (Type 3)",
		{$idJDBC_DRIVER_MERANT_SQL} "Merant JDBC Connect (Type 4)",
		{$idJDBC_DRIVER_MERANT_DB2} "Merant JDBC Connect (Type 4)",
		idJDBC_DRIVER_CUSTOM "Custom Driver"
	]
	DEPENDENCY=[
		DEFAULT={$idJDBC_DRIVER_POSTGRES_ORG} 	IF ^DS_POSTGRESQL,
		DEFAULT={$idJDBC_DRIVER_SQL_INET} 		IF ^DS_SQLSERVER,
		DEFAULT={$idJDBC_DRIVER_ORA_THIN} 		IF ^DS_ORACLE,
		DEFAULT={$idJDBC_DRIVER_IFX_THIN} 		IF ^DS_INFORMIX,
		DEFAULT={$idJDBC_DRIVER_400_TOOLBOX} 	IF (^DS_DB2400 OR ^DS_AS400NATIVE),
		DEFAULT={$idJDBC_DRIVER_DB2_THIN}		IF ^DS_DB2COMMONSERVERS,

		INVALID {$idJDBC_DRIVER_SQL_INET} 		IF NOT ^DS_SQLSERVER,
		INVALID {$idJDBC_DRIVER_SQL_MS} 		IF NOT ^DS_SQLSERVER,
		INVALID {$idJDBC_DRIVER_SQL_WEBLOGIC} 	IF NOT ^DS_SQLSERVER,

		INVALID {$idJDBC_DRIVER_IFX_WEBLOGIC} 	IF NOT ^DS_INFORMIX,
		INVALID {$idJDBC_DRIVER_IFX_THIN} 		IF NOT ^DS_INFORMIX,

		INVALID {$idJDBC_DRIVER_ORA_THIN} 		IF NOT ^DS_ORACLE,
		INVALID {$idJDBC_DRIVER_ORA_INET} 		IF NOT ^DS_ORACLE,

		INVALID {$idJDBC_DRIVER_400_TOOLBOX} 	IF NOT (^DS_DB2400 OR ^DS_AS400NATIVE),
		INVALID {$idJDBC_DRIVER_400_NATIVE}  	IF NOT (^DS_DB2400 OR ^DS_AS400NATIVE),

		INVALID {$idJDBC_DRIVER_DB2_THIN} 		IF NOT ^DS_DB2COMMONSERVERS,
		INVALID {$idJDBC_DRIVER_DB2_TYPE2} 		IF NOT ^DS_DB2COMMONSERVERS,

		INVALID {$idJDBC_DRIVER_MERANT_SQL} 	IF NOT ^DS_SQLSERVER,
		INVALID {$idJDBC_DRIVER_MERANT_DB2} 	IF NOT ^DS_DB2COMMONSERVERS,

		INVALID {$idJDBC_DRIVER_MERANT} IF NOT (^DS_ORACLE OR ^DS_DB2COMMONSERVERS),
		NOT_SHOW IF idDS_ACCESS <> idDS_JDBC
	];

