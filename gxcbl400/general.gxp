MAIN
ONLY_DEFINITION
[
	////////////////////////////////////////////////
	// Definiciones para el control de las propiedades que se muestran

	BOOLEAN
		ID=id_ATT_DESIGN
		NAME="Is Design"
		//APPLY=ATT
		DEFAULT=TRUE
		OPTION=NOT_SHOW;

	BOOLEAN
		ID=id_ATT_ALLOWNULL
		NAME="AllowNull"
		//APPLY=ATT
		DEFAULT=FALSE
		OPTION=NOT_SHOW;

	BOOLEAN
		ID=id_HAS_DOMAIN
		NAME="Has domain"
		APPLY=ATT
		DEFAULT=FALSE
		OPTION=NOT_SHOW;

	COMBO
		ID=id_OBJ_TYPE
		NAME="Object type"
		//APPLY=ATT
		DEFAULT=id_ATT
		DATA=[
			id_OTYPE_ATT	"Attribute",
			id_OTYPE_VAR	"Variable",
			id_OTYPE_DOM	"Domain",
		]
		OPTION=NOT_SHOW;

	COMBO
		ID=id_OBJ_DATATYPE
		NAME="Data type"
		APPLY=ATT
		DEFAULT=id_DT_NUMERIC
		DATA=[
			id_DT_NUMERIC	"Numeric",
			id_DT_CHARACTER	"Character",
			id_DT_VARCHAR	"VarChar",
			id_DT_LONGVARCHAR	"Long Varchar",
			id_DT_DATE		"Date",
			id_DT_DATETIME	"DateTime",
			id_DT_OBJECT	"Object",
		]
		OPTION=READONLY;

	////////////////////////////////////////////////

	TEXT
		ID=id_ATT_VERTICAL_DSC
		NAME="Column Title"
		//APPLY=ATT
		DEFAULT="Default: Attribute Description"
		DEPENDENCY= [
			READONLY IF id_ATT_DESIGN = FALSE,
			NOT_SHOW IF id_OBJ_TYPE = id_OTYPE_VAR OR id_OBJ_TYPE = id_OTYPE_DOM,
		];

	TEXT
		ID=id_ATT_HORIZONTAL_DSC
		NAME="Title"
		//APPLY=ATT
		DEFAULT="Default: Attribute Description"
		DEPENDENCY= [
			READONLY IF id_ATT_DESIGN = FALSE,
			NOT_SHOW IF id_OBJ_TYPE = id_OTYPE_VAR OR id_OBJ_TYPE = id_OTYPE_DOM,
		];

	BOOLEAN
		ID=id_ATT_ISPASSWORD
		NAME="Is Password"
		//APPLY=ATT
		DEFAULT=FALSE
		DEPENDENCY= [
			READONLY IF id_ATT_DESIGN = FALSE
		];
	////////////////////////////////////////////////
	// Propiedades de la picture ///////////////////
	COMBO
		ID=idNUMNUMBERS
		NAME="Left Fill"
		DESC="Left Fill"
		DEFAULT=idNUMNUMBERS_BLANK
		DATA=[
			idNUMNUMBERS_BLANK	"Blank",
			idNUMNUMBERS_ZERO	"Zero",
			idNUMNUMBERS_BZERO	"Blank when Zero",
		]
		DEPENDENCY=[
			NOT_SHOW IF id_OBJ_DATATYPE <> id_DT_NUMERIC,
			//READONLY IF id_ATT_DEF_PICTURE = id_PIC_USER_DEFINED,
			READONLY IF id_ATT_DESIGN = FALSE OR id_HAS_DOMAIN = TRUE,
		];

	BOOLEAN
		ID=idTHOUSANDSEP
		NAME="Thousand Separator"
		DESC="Thousand Separator"
		DEFAULT=FALSE
		DEPENDENCY=[
			NOT_SHOW IF id_OBJ_DATATYPE <> id_DT_NUMERIC,
			//READONLY IF id_ATT_DEF_PICTURE = id_PIC_USER_DEFINED,
			READONLY IF id_ATT_DESIGN = FALSE OR id_HAS_DOMAIN = TRUE,
		];

	TEXT
		ID=idNUMPREFIX
		NAME="Prefix"
		DESC="Prefix"
		DEPENDENCY=[
			NOT_SHOW IF id_OBJ_DATATYPE <> id_DT_NUMERIC,
			//READONLY IF id_ATT_DEF_PICTURE = id_PIC_USER_DEFINED,
			READONLY IF id_ATT_DESIGN = FALSE OR id_HAS_DOMAIN = TRUE,
		];

	COMBO
		ID=idCHARCASE
		NAME="Case"
		DESC="Case"
		DEFAULT=idCHARCASE_NONE
		DATA=[
			idCHARCASE_NONE		"None",
			idCHARCASE_UPPER	"Upper"
		]
		DEPENDENCY=[
			NOT_SHOW IF (id_OBJ_DATATYPE <> id_DT_CHARACTER) AND (id_OBJ_DATATYPE <> id_DT_VARCHAR) 
				AND (id_OBJ_DATATYPE <> id_DT_LONGVARCHAR),
			//READONLY IF id_ATT_DEF_PICTURE = id_PIC_USER_DEFINED,
			READONLY IF id_ATT_DESIGN = FALSE OR id_HAS_DOMAIN = TRUE,
		];

	COMBO
		ID=idDATEFORMAT
		NAME="Date Format"
		DESC="Date Format"
		DEFAULT=idDATEFORMAT_SHORT
		DATA=[
			idDATEFORMAT_NONE	"None",
			idDATEFORMAT_SHORT	"Year with two digits  (99/99/99)",
			idDATEFORMAT_LONG	"Year with four digits (99/99/9999)",
		]
		DEPENDENCY=[
			NOT_SHOW IF (id_OBJ_DATATYPE <> id_DT_DATE) AND (id_OBJ_DATATYPE <> id_DT_DATETIME),
			//READONLY IF id_ATT_DEF_PICTURE = id_PIC_USER_DEFINED,
			READONLY IF id_ATT_DESIGN = FALSE OR id_HAS_DOMAIN = TRUE,
		];

	COMBO
		ID=idHOURFORMAT
		NAME="Hour Format"
		DESC="Hour Format"
		DEFAULT=idHOURFORMAT_HOURMIN
		DATA=[
			//idHOURFORMAT_NONE		"None",
			idHOURFORMAT_ONLYHOUR	"Only hour (hh)",
			idHOURFORMAT_HOURMIN	"Hour and minutes (hh:mm)",
			idHOURFORMAT_HOURMINSEC	"Hour, minutes and seconds (hh:mm:ss)"
		]
		DEPENDENCY=[
			NOT_SHOW IF id_OBJ_DATATYPE <> id_DT_DATETIME,
			//READONLY IF id_ATT_DEF_PICTURE = id_PIC_USER_DEFINED,
			READONLY IF id_ATT_DESIGN = FALSE OR id_HAS_DOMAIN = TRUE,
		];
	////////////////////////////////////////////////

	TEXT
		ID=id_ATT_PICTURE
		NAME="Picture"
		APPLY=ATT
		DEPENDENCY=[
			NOT_SHOW IF ((idNUMNUMBERS=idNUMNUMBERS_BLANK AND idNUMNUMBERS<>idNUMNUMBERS_BLANK) OR
				(idTHOUSANDSEP=TRUE AND idTHOUSANDSEP=FALSE) OR
				(idNUMPREFIX="" AND idNUMPREFIX<>"") OR
				(idCHARCASE=idCHARCASE_NONE AND idCHARCASE<>idCHARCASE_NONE) OR
				(idDATEFORMAT=idDATEFORMAT_SHORT AND idDATEFORMAT<>idDATEFORMAT_SHORT) OR
				(idHOURFORMAT=idHOURFORMAT_HOURMIN AND idHOURFORMAT<>idHOURFORMAT_HOURMIN)),
			NOT_SHOW IF (id_OBJ_DATATYPE=id_DT_OBJECT),
			READONLY IF (id_OBJ_DATATYPE=id_DT_DATE OR id_OBJ_DATATYPE=id_DT_DATETIME) OR (id_ATT_DESIGN = FALSE),
		]
		OPTION=DYN_DEFAULT;
]

