<appender name="CloudWatch" type="AWS.Logger.Log4net.AWSAppender,AWS.Logger.Log4net">

  <LogGroup>$Generator.LOG_GROUP$</LogGroup>
  <Region>$Generator.AMAZON_REGION$</Region>
  <Profile>$Generator.AMAZON_PROFILE$</Profile>
  <layout type="log4net.Layout.PatternLayout">
    <conversionPattern value="%d{ISO8601} [%t] %-5p %c - %m%n" />
  </layout>
</appender>