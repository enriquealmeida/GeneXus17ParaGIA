<BuildSteps>
	<BuildStep Name="Create Deploy Project" MsBuild="deploy.msbuild" Target="CreateDeploy" Verbosity="quiet">
		<Properties>
			<Property>
				<Name>KBPath</Name>
				<Value>$KB.Location$</Value>
			</Property>
			<Property>
				<Name>KBEnvironment</Name>
				<Value>$Environment.Name$</Value>
			</Property>
			<Property>
				<Name>KBVersion</Name>
				<Value>$KBVersion.Name$</Value>
			</Property>
			<Property>
				<Name>DeploymentUnit</Name>
				<Value>$DeploymentUnit$</Value>
			</Property>
			<Property>
				<Name>ProjectName</Name>
				<Value>$DeploymentUnit$_$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>TargetId</Name>
				<Value>$DeployTarget.Name$</Value>
			</Property>
			<Property>
				<Name>ObjectNames</Name>
				<Value>$ObjectsSelection$</Value>
			</Property>
			<Property>
				<Name>ApplicationServer</Name>
				<Value>$DeployTarget.ApplicationServer$</Value>
			</Property>
			<Property>
				<Name>DEPLOY_TARGETS</Name>
				<Value>$DeployTarget.PackageTargets$</Value>
			</Property>
			<Property>
				<Name>EXTRA_MSBUILD</Name>
				<Value>$DeployTarget.ExtraMSBuild$</Value>
			</Property>
			<Property>
				<Name>DeployFullPath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\Deploy\$DeployTarget.Name$\$DeploymentUnit$\$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>WebSourcePath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\web</Value>
			</Property>
		</Properties>
	</BuildStep>
	<BuildStep Name="Create Package" MsBuild="$KB.Location$\$Model.TargetPath$\web\$DeploymentUnit$_$TimeStamp$.gxdproj" Target="CreatePackage" Verbosity="minimal"/>
	<BuildStep Name="Create Cloud Package" MsBuild="CreateCloudPackage.msbuild" Target="CreatePackage" Verbosity="minimal" >
		<Properties>
			<Property>
				<Name>TargetId</Name>
				<Value>$DeployTarget.Name$</Value>
			</Property>
			<Property>
				<Name>CreatePackageScript</Name>
				<Value>$DeployTarget.CreateCloudPackageScript$</Value>
			</Property>
			<Property>
				<Name>CreatePackageTarget</Name>
				<Value>$DeployTarget.CreateCloudPackageTarget$</Value>
			</Property>
			<Property>
				<Name>DeployFullPath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\Deploy\$DeployTarget.Name$\$DeploymentUnit$\$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>WebSourcePath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\web</Value>
			</Property>
			<Property>
				<Name>ProjectName</Name>
				<Value>$DeploymentUnit$_$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>DeploymentUnit</Name>
				<Value>$DeploymentUnit$</Value>
			</Property>
		</Properties>
	</BuildStep>
	<BuildStep Name="Create FrontEnd Project" MsBuild="deploy.msbuild" Target="CreateDeploy" Verbosity="quiet" >
		<Properties>
			<Property>
				<Name>KBPath</Name>
				<Value>$KB.Location$</Value>
			</Property>
			<Property>
				<Name>KBEnvironment</Name>
				<Value>$Environment.Name$</Value>
			</Property>
			<Property>
				<Name>KBVersion</Name>
				<Value>$KBVersion.Name$</Value>
			</Property>
			<Property>
				<Name>DeploymentUnit</Name>
				<Value>$DeploymentUnit$</Value>
			</Property>
			<Property>
				<Name>ProjectName</Name>
				<Value>$DeploymentUnit$_$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>TargetId</Name>
				<Value>$DeployTarget.Name$</Value>
			</Property>
			<Property>
				<Name>ObjectNames</Name>
				<Value>$ObjectsSelection$</Value>
			</Property>
			<Property>
				<Name>ApplicationServer</Name>
				<Value>$DeployTarget.ApplicationServer$</Value>
			</Property>
			<Property>
				<Name>DEPLOY_TARGETS</Name>
				<Value>$DeployTarget.PackageTargets$</Value>
			</Property>
			<Property>
				<Name>EXTRA_MSBUILD</Name>
				<Value>$DeployTarget.ExtraMSBuild$</Value>
			</Property>
			<Property>
				<Name>DeployFullPath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\Deploy\$DeployTarget.Name$\$DeploymentUnit$\$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>WebSourcePath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\web</Value>
			</Property>
		</Properties>
	</BuildStep>
	<BuildStep Name="Create FrontEnd Package" MsBuild="CreateFrontendPackage.msbuild" Target="CreatePackage" Verbosity="minimal" >
		<Properties>
			<Property>
				<Name>DeployFullPath</Name>
				<Value>$KB.Location$\$Model.TargetPath$\Deploy\$DeployTarget.Name$\$DeploymentUnit$\$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>ProjectName</Name>
				<Value>$DeploymentUnit$_$TimeStamp$</Value>
			</Property>
			<Property>
				<Name>GenExtensionName</Name>
				<Value>$DeployTarget.GenExtensionName$</Value>
			</Property>
			<Property>
				<Name>DeploymentScript</Name>
				<Value>$DeployTarget.DeploymentScript$</Value>
			</Property>
		</Properties>
	</BuildStep>
</BuildSteps>